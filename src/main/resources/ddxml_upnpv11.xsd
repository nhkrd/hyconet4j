                </xsd:complexType>
            </xsd:element>
            <xsd:element name="iconList" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="icon" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
